{
    "agent": "ddpg",
    "states": {
        "type": "float",
        "shape": [
            5
        ],
        "min_value": [
            0.0,
            "-Infinity",
            "-Infinity",
            0.0,
            "-Infinity"
        ],
        "max_value": [
            96.0,
            "Infinity",
            "Infinity",
            1.0,
            "Infinity"
        ]
    },
    "actions": {
        "type": "float",
        "shape": 1,
        "max_value": 0.5,
        "min_value": -0.5
    },
    "max_episode_timesteps": 96,
    "batch_size": 256,
    "network": [
        {
            "type": "dense",
            "size": 200,
            "activation": "relu"
        },
        {
            "type": "dense",
            "size": 400,
            "activation": "relu"
        }
    ],
    "memory": 1024,
    "start_updating": 512,
    "update_frequency": 96,
    "learning_rate": 1e-4,
    "discount": 0.95,
    "return_processing": null,
    "predict_terminal_values": false,
    "critic": [
        [
            {
                "type": "retrieve",
                "name": "states_retrieve",
                "tensors": [
                    "states"
                ]
            },
            {
                "type": "dense",
                "size": 200,
                "activation": "relu"
            },
            {
                "type": "linear",
                "size": 400
            },
            {
                "type": "register",
                "name": "states_register",
                "tensor": "states-embedding"
            }
        ],
        [
            {
                "type": "retrieve",
                "name": "actions_retrieve",
                "tensors": [
                    "actions"
                ]
            },
            {
                "type": "linear",
                "size": 400
            },
            {
                "type": "register",
                "name": "actions_register",
                "tensor": "actions-embedding"
            }
        ],
        [
            {
                "type": "linear",
                "size": 400
            },
            {
                "type": "retrieve",
                "name": "retrieve",
                "tensors": [
                    "states-embedding",
                    "actions-embedding"
                ],
                "aggregation": "concat",
                "axis": 0
            }
        ]
    ],
    "critic_optimizer": {
        "optimizer": "adam",
        "learning_rate": 1e-3
    },
    "exploration": 0.1,
    "variable_noise": 0.1,
    "l2_regularization": 0.0,
    "entropy_regularization": 0.0,
    "parallel_interactions": 1,
    "config": null,
    "saver": {
        "directory": "saver_dir",
        "frequency": 100,
        "unit": "episodes"
    },
    "summarizer": {
        "directory": "summaries",
        "summaries": "all"
    },
    "tracking": null,
    "recorder": null
}